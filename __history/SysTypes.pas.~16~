unit SysTypes;

interface

uses
  System.JSON, IdCustomHTTPServer;

Type
  TResultErro = record
     STATUS        : Integer;
     MENSAGEM      : String;
  end;

  TRequisicao = class
    link : string;
    tipoRequisicao : THTTPCommandType;
    bodyParams : string;
  end;

  TArguments = array of string;

implementation

end.
