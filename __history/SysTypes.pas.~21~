unit SysTypes;

interface

uses
  System.JSON, IdCustomHTTPServer;

Type
  TResultErro = record
     STATUS        : Integer;
     MENSAGEM      : String;
  end;

  TRequisicao = class
    URL : string;
    Tipo : THTTPCommandType;
    queryParams : string;
    bodyParams : string;

  end;

  TArguments = array of string;

implementation

end.
